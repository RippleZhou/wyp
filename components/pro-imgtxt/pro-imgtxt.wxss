/* components/ pro-imgtxt/ pro-imgtxt.wxss */
.w_proImgTxt_luls {
  display: -webkit-box;
  display: flex;
  display: -ms-flexbox;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  width: 100%;
}
.w_proImgTxt_list {
  width: 49.3%;
  background: #fff;
  margin-bottom: 10rpx;
  margin-right: .7%;
}
.w_proImgTxt_list_img {
  width: 100%;
  border-bottom: 1px solid #ebebeb;
  line-height: 0;
  display: block;
}
.w_proImgTxt_l_txt {
  padding: 20rpx;
  text-align: left;
}
.w_proImgTxt_l_txt_h2 {
  font-size: 30rpx;
  color: #333;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.w_proImgTxt_links {
  padding: 15rpx 0;
  display: -webkit-box;
  display: flex;
  display: -ms-flexbox;
}
.w_proImgTxt_links_a {
  color: #e61817;
  font-size: 24rpx;
  display: block;
  width: auto;
  margin-right: 15rpx;
  padding: 0 10rpx;
  border: 1px solid #e61817;
  height: 32rpx;
  border-radius: 32rpx;
  line-height: 30rpx;
  text-align: center;
}
.w_yelTxts {
  color: #ff9000;
  border: 1px solid #fccf95;
}
.w_orangeTxts {
  color: #fdcc07;
  border: 1px solid #fdcc07;
}
.loading-layer {
  width: 100%;
  height: 60px;
  text-align: center;
  font-size: 16px;
  line-height: 60px;
  color: #aaa;
  position: relative;
}
