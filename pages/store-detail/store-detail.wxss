view,text,label,page{box-sizing: border-box;-webkit-box-sizing: border-box;}
.popup-wrap {
  display: block;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  opacity: 1;
  z-index: 100;
  -webkit-transition: opacity 400ms;
  transition: opacity 400ms;
}
.popup-content {
  background-color: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 501;
  height:800rpx;
  transition-property: transform;
  transition-duration: 300ms;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
}
.w_popColsed {
  position: absolute;
  right: 0;
  top: 0;
  display: block;
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  z-index: 560;
}
.w_popColsed::before {
  font-family: iconfont!important;
  font-weight: 400;
  font-style: normal;
  content: "\e70b";
  color: #666;
  font-size:50rpx;
  vertical-align: middle;
}
.w_pop_proImg {
  width: 100%;
  border-bottom: 1px solid #ebebeb;
  padding: 20rpx;
  padding-bottom: 50rpx;
  display: flex;
}
.popup-product-img {
  width: 218rpx;
  height: 218rpx;
  margin-right: 20rpx;
}
.w_pop_riPric {
  padding-top: 110rpx;
}
.w_pop_riPric_h2 {
  font-size: 30rpx;
  color: #e61817;
}
.w_pop_pric_span {
  display: block;
  font-size: 24rpx;
  color: #333;
}
.w_pop_specs {
  padding: 35rpx 10rpx;
  padding-top: 60rpx;
  border-bottom: 1px solid #ebebeb;
  display: flex;
  flex-wrap: wrap;
}
.sku-item {
  display: block;
  font-size: 28rpx;
  color: #333;
  background: #ebebeb;
  border: 1px solid #ebebeb;
  height: 60rpx;
  padding: 0 30rpx;
  text-align: center;
  border-radius: 10rpx;
  margin-right: 10rpx;
  margin-left: 10rpx;
  line-height: 56rpx;
  margin-bottom: 20rpx;
}
.w_tabSpanCur {
  background: #fef1f6;
  border: 1px solid #e61817;
  color: #e61817;
}
.w_pop_selNums {
  padding: 30rpx 20rpx;
  padding-bottom: 130rpx;
  display: -webkit-box;
  display: flex;
}
.pop_label {
  font-size: 30rpx;
  color: #333;
  flex-grow: 1;
  text-align: left;
}
.w_pop_selBtns {
  width: 220rpx;display:flex;display:-webkit-flex; display: -ms-flexbox;
  position: relative;
}
.icon-sub, .icon-add {
  margin: auto;
}

.icon-sub::before {
  font-family: iconfont !important;
  content: "\e612";
  line-height: 30rpx;
  float: left;
  display: block;
  font-size: 50rpx;
}
.icon-add::before {
  font-family: iconfont !important;
  content: "\e634";
  line-height: 30rpx;
  float: left;
  display: block;
  font-size: 50rpx;
  margin: auto;
}
.input-num {
  width: 100rpx;
  text-align: center;
  font-size: 20px;
  color: #333;
}
.w_popBtnfot {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  display: flex;z-index: 500;
}

.w_popBtnfot_span,.w_popBtnfot_span2 {
  display: block;
  width: 50%;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  color: #fff;
  background: #2d2f36;
}
.w_popBtnfot_span2{width:100%;}
.w_popBtnReds {
  background: linear-gradient(to right,#f90000,#f61e65);
}
.prevent-icon {
  opacity: 0.2
}
.fix-iphonex-button{
  bottom: 28rpx!important;
}
.fix-iphonex-button::after {
  content: '';
  position: fixed;
  bottom: 0!important;
  width: 100%;
  background: #fff;
} 
