/*搜索图标*/

.scroll_top_search {
  font-size: 20rpx;
  position: absolute;
  top: 0;
  left: 20rpx;
}

/*搜索图标*/

.scroll_top_search:before {
  display: inline-block;
  content: '\e625';
  font-size: 40rpx;
  font-family: iconfont !important;
  vertical-align: middle;
  margin-top: 0;
  margin-right: 24rpx;
}

/*扫一扫图标*/

