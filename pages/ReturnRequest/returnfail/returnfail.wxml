<!--pages/ReturnRequest/rerurnSuccess/rerurnSuccess.wxml-->
<view class='pay_fail'>
    <view class='fail_img'>
      <image src='/img/payfail.png'></image>
    </view>
    <view class='fail_txt font_34'>
      支付失败
    </view>
    <view class='btn_list flex flex-main-center'>
      <view class='look_order' bindtap='LookOrder'>查看订单</view>
      <view class='re_pay' bindtap='rePay'>重新支付</view>
    </view>
</view>
