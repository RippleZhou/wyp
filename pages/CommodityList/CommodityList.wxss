/* pages/CommodityList/CommodityList.wxss */
@import '../../style/flex.wxss';
@import '../../style/common.wxss';
page{
  background: #f6f6f6;
}
.commodity_top{
  border-top: 1px solid #e6e6e6;
}
.commodity_img{
  height: 154rpx;
  width:154rpx;
  background: #cecece;
  margin-right: 24rpx;
}
/* 自动换行第二行显示超出后省略 */
.title_clamp{
overflow:hidden;
text-overflow:ellipsis;
display:-webkit-box;
-webkit-line-clamp:2;
-webkit-box-orient:vertical;
}
.commodity_price{
  text-align:right;
}
.flexbox-3{
  flex-shrink:32;
}