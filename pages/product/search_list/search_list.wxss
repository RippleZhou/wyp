/* pages/product/search_list/search_list.wxss */
@import "/style/common.wxss";
.head_index {
  z-index: 100;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  width: 100%;
  background: #fff;
  height: 88rpx;
  border-bottom: 1px solid #f5f5f5;
}
.head_i_cens {
  width: 80%;
  background: #f5f5f5;
  height: 60rpx;
  border-radius: 66rpx;
  margin: 14rpx 20rpx 0;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.head_i_cens:before {
  line-height: 66rpx;
  margin-right: 12rpx;
  display: inline-block;
  font-family: iconfont !important;
  font-weight: 400;
  font-style: normal;
  content: "\e625";
  color: #666;
  font-size: 40rpx;
  vertical-align: top;
}
.search_form {
  display: inline-block;
  width: 90%;
  height: 60rpx;
}
.search_input {
  font-size: 26rpx;
  color: #999;
  vertical-align: middle;
  height: 60rpx;
  width: 100%;
}
.w_head_rightTxt {
  width: 15%;
  position: absolute;
  top: 15rpx;
  font-size: 30rpx;
  color: #666;
  line-height: 60rpx;
  text-align: right;
  right: 20rpx;
  z-index: 999;
}
.w_head_rightTxt::before {
  line-height: 60rpx;
  margin-right: 12rpx;
  display: inline-block;
  font-family: iconfont !important;
  font-weight: 400;
  font-style: normal;
  content: "\e61d";
  color: #666;
  font-size: 40rpx;
  vertical-align: top;
}
.product-list {
  margin-top: 90rpx;
  height: calc(100vh - 90rpx);
}
.no_blankIms {
  width: 30%;
  display: block;
  margin: 0 auto;
  padding-top: 100rpx;
}
.nodata-text {
  width: 100%;
  height: 60px;
  text-align: center;
  font-size: 32rpx;
  line-height: 120rpx;
  color: #aaa;
  position: relative;
}
.mall_box {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
}

.mall_list {
  margin-left: 2%;
  background: #fff;
  width: 48%;
  margin-top: 20rpx;
  padding-bottom: 10rpx;
}

.mall_pd {
  margin: 0 20rpx;
}
.mall_t_img {
  width: 100%;
  height: 350rpx;
  border-bottom: 1px solid #ebebeb;
  overflow: hidden;
}
.mall_t_img image {
  width: 100%;
}
.mall_t_tit {
  font-size: 30rpx;
  color: #333;
  padding-top: 20rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  -webkit-line-clamp: 2;
}
.mall_t_tip {
  font-size: 24rpx;
  color: #e61817;
  padding: 0 20rpx;
  background: #ffedec;
  border-radius: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
}
.mall_t_pirce {
  font-size: 24rpx;
  color: #e61817;
}
.mall_t_pirce text {
  font-size: 36rpx;
}
.mall_t_pirce label {
  color: #999;
  text-decoration: line-through;
}
.mall_t_nums {
  color: #999;
  font-size: 24rpx;
}
