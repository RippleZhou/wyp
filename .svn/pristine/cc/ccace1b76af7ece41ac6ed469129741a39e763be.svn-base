/* pages/bills/bills.wxss */
@import '../../style/flex.wxss';
@import '../../style/common.wxss';
page{
    background:#f6f6f6 ;
    height:100%;
}
.bills{
    background: #fff;
    margin:0 auto;
    height:100%;
    margin-top:10rpx;
    padding: 0 24rpx;
}
.bill_title{
    padding-top:100rpx;
}
.bill_price{
    margin-top:56rpx;
}
.bill_status{
    margin-top:30rpx;
}
.flex_56{
    display: flex;
    height: 56rpx;
    line-height: 56rpx;
}
.flex_70{
    display: flex;
    height: 70rpx;
    line-height: 70rpx;
}
.mar_0{
    width: 100%;
    margin:0 auto;
    text-align: center;
}
.col_555{
    color: #b5b5b5;
}
.txt_l{
    text-align: left;
    display: inline-block;
}
.txt_r{
    text-align: right;
    display: inline-block;

}

.bor_dash{
    border-bottom: 1px dashed #b5b5b5;
    border-top: 1px dashed #b5b5b5;

}
.h56{
    height: 56rpx;
    line-height: 56rpx;
}
.pad_24_0{
    padding:24rpx 0;
}
