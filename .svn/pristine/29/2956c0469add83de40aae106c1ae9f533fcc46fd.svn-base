/* pages/shoppingcar/shoppingcar.wxss */
@import '../../style/flex.wxss';
@import '../../style/common.wxss';
page{
  background: #f6f6f6;
}
.pages{
  padding:10rpx 24rpx;
}
/* 列表 */
.list{
    position: relative;
    width: 100%;
    padding: 24rpx;
    border-bottom: 1rpx solid #e9e9e9;
    border-radius: 50rpx;
}
/* 编辑 */
.edit{
  position: relative;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #f6f6f6;
}
/* 删除 */
.dele_edit{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  right: 30rpx;
  bottom: 25rpx;
}
/* 编辑标题 */
.edit_btn{
  position: absolute;
  font-size: 26rpx;
  color: #888;
  right: 30rpx;
}
/* 单选状态 */
.radio_chek{
    display:inline-block;
    border-radius:50%;
}
/* 列表商品图片 */
.list_img{
    border-radius:30rpx;
    width: 184rpx;
    height: 184rpx;
    margin-right:24rpx;
}
.list_img image{
  height: 100%;
  width: 100%;
}
/* 列表规格 */
.cart_g_name{
  left: 280rpx;
  font-size: 26rpx;
  color: #aaa;
  top: 75rpx;
}

/* 列表商品名称 */
.list_name{
  position: absolute;
  left: 280rpx;
  top: 30rpx;
  width: 300rpx;
  font-size: 30rpx;
  overflow: hidden;    
  text-overflow:ellipsis;    
  white-space: nowrap;
}
/* 列表商品价格 */
 .list_price{
position: absolute;
font-size: 32rpx;
color:  #D66058;
left: 280rpx;
bottom: 20rpx;
}
.list .icon{
  display:flex;
  align-items:center;
  margin:0 24rpx;
}
 
/* 列表商品删除 */
.list_del{
    position: absolute;
    right: 220rpx;
    bottom: 20rpx;
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    font-size: 44rpx;
}

.cart{
    padding-bottom: 100rpx;
    font-size: 26rpx;
}
.item{
  border-bottom: 1px solid #eee;
}
/* 头部店铺信息 */
.storeInfo{
  display: flex;
  padding: 18rpx 0rpx 18rpx 30rpx;
  background: #fff;
  box-sizing: border-box;
}
.storeInfo .avatar{
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.storeInfo .storeName{
  margin-left: 16rpx;
  line-height: 56rpx;
}
/* 包邮信息 */
.discount{
  padding-left: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 20rpx;
  color: #666;
  box-sizing: border-box;
}
/* 底部操作 */
.count{
    display: flex;
    position: fixed;
    padding-left: 30rpx;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 98rpx;
    line-height: 80rpx;
    box-sizing: border-box;
    color: #232323;
    background: #fff;
    padding: 9rpx 24rpx;
}
/* 全选 */
.selectAll{
  float: left;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  width: 208rpx;
}
.selectAll .noSelected{
  display:inline-block;
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  border: 1px solid #cecece;
}
.SelectedAll{
  display:inline-block;
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  border: 1px solid #cecece;
  background: #fa0000;
}
.selectAll .selectedImg{
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  border: 1px solid #cecece;
}
.selectAllText{
    margin-left: 18rpx;
}

.countPrice{
  width: 274rpx;
  float: left;
  text-align: center;
  font-size: 30rpx;
}
.countPrice view{
  height: 40rpx;
  line-height: 40rpx;
}
.countPrice text{
  margin-right: 15rpx;
}
.account{
  float: right;
height: 80rpx;
line-height: 80rpx;
width: 222rpx;
text-align:center;
box-sizing:border-box;
color:#fff;
background:#e61817;
border-radius: 50rpx;
}
/* 购物车为空 */
.list_none{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}
/* 提交 */
.submit{
position: absolute;
width: 232rpx;
height: 104rpx;
line-height: 104rpx;
right: 0rpx;
font-size: 30rpx;
text-align: center;
color:#fff;
background-color:#fa0000;
}
.navigator-hover{
    background: none;
}

 
/*数量加减*/
.carts-num{
  position: absolute;
  right: 30rpx;
  bottom: 23rpx;
  /* background-color: red; */
  display: flex; align-items: center; text-align: center; height: 50rpx;}
.view_text_center{border: 1rpx solid #ebebeb;font-size: 30rpx;  
display: inline-block;color: #333;height: 50rpx;line-height: 50rpx; width: 76rpx;}
.carts-num .minus, .carts-num .plus{margin: 10rpx; border: 1rpx solid #ebebeb;color: #000; text-align: center;height: 50rpx;  line-height: 50rpx; width: 40rpx;}
.carts-num .minus{font-size: 34rpx;}
/* .carts-num .minus.disabled{color: red;} */
.carts-num .plus{font-size: 34rpx;}
