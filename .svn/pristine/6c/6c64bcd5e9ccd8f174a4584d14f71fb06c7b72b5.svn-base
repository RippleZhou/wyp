/* pages/shoppingcar/shoppingcar.wxss */
@import '../../style/flex.wxss';
@import '../../style/common.wxss';
page{
  background: #f6f6f6;
}
.pages{
  position: relative;
  box-sizing: border-box;
  padding:10rpx 24rpx;
  padding-bottom: 56px;
}
/* 列表 */
.list{
    padding: 24rpx;
    border-bottom: 1rpx solid #e9e9e9;
    background:#fff;
}
/* 编辑 */
.edit{
  position: reslative;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #f6f6f6;
}
/* 删除 */
.dele_edit{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  right: 30rpx;
  bottom: 25rpx;
}
/* 编辑标题 */
.edit_btn{
  position: absolute;
  font-size: 26rpx;
  color: #888;
  right: 30rpx;
}
/* 单选状态 */
.radio_chek{
    display:inline-block;
    border-radius:50%;
}
/* 列表商品图片 */
.list_img{
    border-radius:30rpx;
    width:240rpx;
    height:184rpx;
    overflow:visibility;
}
.list_img image{
  height:184rpx;
  width:184rpx;

}
/* 列表商品名称 */
.list_name{
  font-size: 26rpx;
  overflow: hidden;   
}
/* 列表商品价格 */
 .list_price{
font-size: 26rpx;
color:  #fa0000;
left: 280rpx;
bottom: 20rpx;
}
.list .icon{
  width:60rpx;
  display:flex;
  align-items:center;
}
 
/* 列表商品删除 */
.list_del{
    right: 220rpx;
    bottom: 20rpx;
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    font-size: 44rpx;
}

.cart{
    padding-bottom: 100rpx;
    font-size: 26rpx;
}
.item{
  border-bottom: 1px solid #eee;
}
/* 头部店铺信息 */
.storeInfo{
  display: flex;
  padding: 18rpx 0rpx 18rpx 30rpx;
  background: #fff;
  box-sizing: border-box;
}
.storeInfo .avatar{
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.storeInfo .storeName{
  margin-left: 16rpx;
  line-height: 56rpx;
}
/* 包邮信息 */
.discount{
  padding-left: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 20rpx;
  color: #666;
  box-sizing: border-box;
}
/* 底部操作 */
.count{
    display: flex;
    position: fixed;
    padding-left: 30rpx;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 98rpx;
    line-height: 80rpx;
    box-sizing: border-box;
    color: #232323;
    background: #fff;
    padding: 9rpx 24rpx;
}
/* 全选 */
.selectAll{
  float: left;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  width: 208rpx;
}
.selectAll .noSelected{
  display:inline-block;
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  border: 1px solid #cecece;
}
.SelectedAll{
  display:inline-block;
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  border: 1px solid #cecece;
  background: #fa0000;
}
.selectAll .selectedImg{
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  border: 1px solid #cecece;
}
.selectAllText{
    margin-left: 18rpx;
}

.countPrice{
  width: 274rpx;
  float: left;
  text-align: center;
  font-size: 30rpx;
}
.countPrice view{
  height: 40rpx;
  line-height: 40rpx;
}
.countPrice text{
  margin-right: 15rpx;
}
.account{
float: right;
height: 80rpx;
line-height: 80rpx;
width: 222rpx;
text-align:center;
box-sizing:border-box;
color:#fff;
background:#e61817;
border-radius: 50rpx;
}
.wyb_account{
  height:40rpx;
  line-height:40rpx;
}
/* 购物车为空 */
.list_none{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}
/* 提交 */
.submit{
width: 232rpx;
height: 80rpx;
line-height: 80rpx;
font-size: 30rpx;
text-align: center;
color:#fff;
background-color:#fa0000;
}
.navigator-hover{
    background: none;
}
.count .radio_chek{
  margin-top:32rpx;
margin-right:16rpx;

}

/*数量加减*/
.carts-num{
  display: flex; align-items: center; text-align: center; height: 50rpx;margin-top:33rpx;}
.view_text_center{font-size: 30rpx;  
display: inline-block;color: #333;height: 50rpx;line-height: 50rpx; width: 76rpx;}
.carts-num .minus{margin-left:144rpx;}
.carts-num .minus,.carts-num .plus{ font-size:30rpx;display:inlien-block;width:50rpx; height:50rpx;border-radius:50%;border:1px solid #d3d3d3;line-height: 45rpx;text-align: center;}
.carts-num .minus.active,.carts-num .plus.active{
  border:1px solid #343434;
}