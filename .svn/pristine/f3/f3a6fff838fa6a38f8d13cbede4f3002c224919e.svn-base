<!--pages/order/exchangeBox.wxml-->
<view class='Boxs'>
  <view class='marBoxs'>
    <view wx:for="{{ebArray}}" wx:for-index="i" wx:for-item="ebItems" bindtouchstart="removestart" bindtouchmove="removeload" bindtouchend="removeend" data-id="{{ebItems.delIndex}}" class="eb-lCons {{(ebItems.delIndex==ebtId)?'ebItems-del':''}}">
      <view class='wx_swiper'>
        <view class='wx_swiper_left'>
          <view class='checkSels '>
            <navigator class='checkselsel'></navigator>
          </view>
        </view>
        <image></image>
        <view class='w_exLi_info'>
          <text>盼盼法式小面包1.5kg软面包面包 面包面包面包</text>
          <view></view>
          <view class="w_pop_selBtns">
            <label>¥111</label>
            <view class='w_Btns'>
              <view class="subBtn {{ebItems.eNums<=0?'subBtnNos':''}}" catchtap="sub" id="info{                             {ebItems.eIndex}}"></view>
              <input type="number" value="{{ebItems.eNums}}" class='inputnum' />
              <view class="addBtn" catchtap="add" id="info{{ebItems.eIndex}}"></view>
            </view>
          </view>
        </view>

      </view>
      <view class="wx_car_del" data-id="{{ebItems.delIndex}}" catchtap="delcart" catchtap="delcart">
        删除
      </view>
    </view>
  </view>
  <view class='car_footers'>
    <view class='w_car_sels'>
      <view class='checkSels '>
        <navigator class='checkselsel'></navigator>

      </view>
      全选
    </view>
    <view class='w_car_price'>
      <label class='w_pricetxt'>
        合计：
      </label>
      <label class='w_price'> ¥120</label>
      <text class='w_car_txt'>
        可获物业币120个
        </text>
    </view>
    <navigator class='w_a_nav'>去结算</navigator>
  </view>
  <view class='car_footer'>
  </view>
</view>